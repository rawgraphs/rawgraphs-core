    "@rollup/plugin-json": "^4.0.3",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.2",
    "d3-selection": "^1.4.2",
    "dayjs": "^1.8.22",
    "docdash": "^1.2.0",
